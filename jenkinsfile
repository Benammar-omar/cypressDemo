pipeline{
    agent any 

    parameters{
        string(name:'SPEC',defaultValue : "cypress/integration/**/**",description: "Enter the script")
        choice(name:'Browser',choices :['chrome','edge'],description : "choice the browser")
    }
    stages{
    stage('Deployin'){
        echo "Building the application"
    }
    stage('Testing'){
        steps{
            bat"npn i"
            bat "npx cypress run"
        }
    }
    }
}